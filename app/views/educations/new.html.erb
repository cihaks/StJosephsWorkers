<% form_for(@education) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :list_order %><br />
    <%= f.text_field :list_order %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

