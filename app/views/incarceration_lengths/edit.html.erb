<% form_for(@incarceration_length) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :list_order, "List Order" %><br />
    <%= f.text_field :list_order, :size=>"4", :maxlength=>"2" %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<br />