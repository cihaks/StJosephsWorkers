
<p>
  <%= f.label :company_id %><br />
  <%= f.text_field_with_auto_complete :company_name, {}, { :url => companies_path(:ac), :method => :get, :param_name => 'query' } %>
</p>
<p>
  <%= f.label :industry_id %><br />
  <%= f.collection_select(:industry_id, Industry.all, :id, :name) %>
</p>
