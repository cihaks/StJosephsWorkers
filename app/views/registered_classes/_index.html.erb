
<table>
  <tr>
    <th>Class</th>
	<td width="10"></td>
    <th>Class date</th>
	<td width="10"></td>
    <th>Waive date</th>
	<td width="10"></td>
    <th>Completed</th>
	<td width="10"></td>
  </tr>

<% @client.registered_classes.each do |registered_class| %>
  <tr>
    <td><%=h registered_class.course.name %></td>
	<td width="10"></td>
    <td><%=h registered_class.class_date.strftime("%m/%d/%Y") unless registered_class.class_date.nil? %></td>
	<td width="10"></td>
    <td><%=h registered_class.waive_date.strftime("%m/%d/%Y") unless registered_class.waive_date.nil? %></td>
	<td width="10"></td>
    <td align="center"><%= check_box_tag(:completed,'true',registered_class.completed, :onclick=>"return false;") %></td>
	<td width="10"></td>
    <td><%= link_to_remote 'Details', :url=>client_registered_class_path(@client,registered_class), :update=>'jobs-content', :method=>:get %></td>
    <td><%= link_to_remote 'Edit', :url=>edit_client_registered_class_path(@client,registered_class), :update=>'jobs-content', :method=>:get %></td>
    <td><%= link_to_remote 'Destroy', :url=>client_registered_class_path(@client,registered_class), :update=>'jobs-content', :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
<% if @client.registered_classes.empty? %>
  <tr>
  	<td colspan="5" class="fieldData">No Registered Classes</td>
  </tr>
<% end %>
</table>

<br />
