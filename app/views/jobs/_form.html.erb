<table>
	<tr>
	  <td valign="top">
		  <p>
		    <%= f.label :job_type_id, 'Job Type' %><br />
		    <%= f.collection_select(:job_type_id, JobType.all, :id, :name) %>
		  </p>
		  <p>
		    <%= f.label :company_id, 'Company' %><br />
		    <%= f.text_field_with_auto_complete :company_name, {}, { :url => companies_path(:ac), :method => :get, :param_name => 'query' } %>
		  </p>
		  <p>
		    <%= f.label :industry_id, 'Industry' %><br />
		    <%= f.collection_select(:industry_id, Industry.all, :id, :name) %>
		  </p>
		  <p>
		    <%= f.check_box :current %>
		    <%= f.label :current %>
		  </p>
		  <p>
		    <%= f.label :last_verified_date, "Last Verified Date" %><br />
		    <%= f.calendar_date_select :last_verified_date %>
		  </p>
		  <p>
		    <%= f.label :start_date %><br />
		    <%= f.calendar_date_select :start_date %>
		  </p>
		  <p>
		    <%= f.label :end_date %><br />
		    <%= f.calendar_date_select :end_date %>
		  </p>
    </td>
	  <td width="15"></td>
	  <td valign="top">
	
		  <div id="job_rates-element">
			<% f.fields_for :job_rates do |job_rate_fields| %>
			  <%= render "job_rates/form", :f => job_rate_fields %>
			<% end %>
		  </div>
		  <div><%= add_element_link "Add a rate", f, :job_rates %></div>
		  
      </td>
	  <td width="15"></td>
	  <td valign="top">
		<p>
		  <%= f.label :benefits %><br />
		  <%= f.collection_select(:benefits_type_id,BenefitsType.all,:id,:name) %>
		</p>
    </td>
  </tr>
</table>